{
  "name": "lsst-ccs_database",
  "version": "3.1.0",
  "author": "AURA/LSST/SLAC",
  "summary": "CCS specific database configuration",
  "license": "Apache-2.0",
  "source": "https://github.com/lsst-it/puppet-ccs_database",
  "dependencies": [
